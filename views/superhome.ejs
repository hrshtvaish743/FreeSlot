<% include header.ejs %>
<body>
	<% include navbar.ejs %>
<div class="container">
	<div class="page-header text-center">
		<h1><strong>Super User's</strong> Panel</h1>
		<h1><span class="glyphicon glyphicon-home"></span>  Welcome <strong>Harshit Vaish</strong></h1>
	</div>
</div>
	<div class="row">
		<div class="container">
			<div class="well">
					<div class="modal-body row">
						<div class="col-md-8">
							<p>
								List of your Clubs/Chapters/Team Registered on FreeSlot.
							</p>
              <ol>
								<% reg.forEach(function(club) { %>
									<li><%= club.name %> - Verification: <%= club.verified %></li>
									<% }); %>
							</ol>
						</div>
  					<div class="col-md-4">
								<h3><span class="glyphicon glyphicon-wrench"></span> Actions</h3>
								<p>
									<a href="/superuser/verify" class="btn btn-default" role="button">Verify Accounts</a>
							</p>
							<p>
								<a href="/superuser/details" class="btn btn-default" role="button">Get Details</a>
							</p>
						</div>
					</div>
			</div>
		</div>
	</div>
	<% include footer.ejs %>
</body>
</html>
